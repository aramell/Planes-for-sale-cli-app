(function(n){n(".watchlist-add, .watchlist-remove").click(function(t){t.preventDefault();var i=n(this);i.hasClass("watchlist-add")?n.ajax({url:i.attr("href"),method:"POST",success:function(){i.removeClass("watchlist-add").addClass("watchlist-remove").html('<span class="fa fa-remove m-right-6"><\/span> '+GetTranslation("RemoveFromWatchlist","Remove From Watchlist")).attr("href",i.attr("href").replace("/add/","/remove/"))}}):n.ajax({url:i.attr("href"),method:"POST",success:function(){i.removeClass("watchlist-remove").addClass("watchlist-add").html('<span class="fa fa-plus m-right-6"><\/span> '+GetTranslation("AddToWatchlist","Add To Watchlist")).attr("href",i.attr("href").replace("/remove/","/add/"));~window.location.href.indexOf("/listings/watchlist")&&n('.listing[data-listing-id="'+i.attr("data-listing-id")+'"][data-listing-type="'+i.attr("data-listing-type")+'"][data-basecatid="'+i.attr("data-basecatid")+'"]').remove()}})})})(jQuery)