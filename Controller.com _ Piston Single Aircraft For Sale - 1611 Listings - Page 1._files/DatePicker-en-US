window.Overrides||(window.Overrides={});$.extend(window.Overrides,{baseUrl:window.Overrides.baseUrl?window.Overrides.baseUrl:""});$(document).ready(function(){function n(n){$(n.target||n).each(function(n,t){var i=new Date(Date.parse(t.value)+(new Date).getTimezoneOffset()*6e4);i.valueOf()&&$(t).datepicker("setDate",i)})}$(".datefield").datepicker({showOn:"both",buttonImage:window.Overrides.baseUrl+"/Content/images/calendar-icon.png",buttonImageOnly:!0,changeMonth:!0,changeYear:!0,numberOfMonths:1,dayNamesMin:["Su","Mo","Tu","We","Th","Fr","Sa"],dateFormat:"MM d, yy",showAnim:"show"}).change(function(t){$((t.originalEvent||t).target).one("focusout",n)}).keyup(function(t){(t.keyCode===13||t.keyCode===9)&&n(t)}).each(function(t,i){n(i)})})